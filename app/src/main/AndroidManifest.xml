<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nikunj.svnitchapters">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".svnitchapters"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".signup" />
        <activity
            android:name=".Chapters"
            android:label="@string/title_activity_chapters"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AboutUs" />

        <service android:name=".MyFireBaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <activity android:name=".GDG" />
        <activity android:name=".activepost"></activity>
    </application>

</manifest>